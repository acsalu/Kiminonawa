{
  "name": "Kiminonawa",
  "version": "1.0",
  "manifest_version": 2,
  "description": "",
  "background": {
      "scripts": [
        "./js/background.js"
      ],
      "persistent": false
  },
  "content_scripts": [
  	{
  		"matches": ["http://*/*", "https://*/*"],
  		"js": ["./js/jquery-3.1.1.min.js",
          "./js/ContactParser.js",
          "./js/content.js"
        ],
  		"css": ["./css/style.css"],	
  		"run_at": "document_end"
  	}
  ],
  "web_accessible_resources": [
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "<all_urls>"
  ]
}

